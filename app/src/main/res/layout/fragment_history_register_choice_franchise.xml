<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>

    </data>

    <LinearLayout
        android:id="@+id/fragment_history_register_choice_franchise_parent_linear_layout"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_main_white">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:isScrollContainer="false"
            android:overScrollMode="never"
            android:scrollbars="none"
            android:clipToPadding="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginTop="10dp"
                    android:layout_margin="16dp"
                    android:background="@drawable/background_radius_10dp_white_box"
                    android:elevation="6dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/icon_search_magni_class" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="24dp"
                        android:layout_gravity="center"
                        android:gravity="center|start"
                        android:background="@android:color/transparent"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:textColor="@color/color_text_jade_green"
                        android:textCursorDrawable="@drawable/edit_text_cursor_drawable"
                        android:hint="@string/fragment_history_today_register_search_text"
                        android:textColorHint="@color/color_text_edit_text_hint"
                        android:maxLength="50"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:includeFontPadding="false"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/background_radius_10dp_white_box"
                    android:padding="24dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    android:elevation="6dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_1"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/background_layer_history_register_franchise"
                            android:contentDescription="@string/image_view_description_choice_franchise_1"
                            app:layout_constraintHorizontal_chainStyle="spread_inside"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_2" />

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_2"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:scaleType="centerCrop"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_2"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_1"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_3"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_3"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_3"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_2"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_4"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_4"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_4"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_3"
                            app:layout_constraintRight_toRightOf="parent"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:layout_marginTop="24dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_5"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_5"
                            app:layout_constraintHorizontal_chainStyle="spread_inside"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_6" />

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_6"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_6"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_5"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_7"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_7"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_7"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_6"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_8"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_8"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_8"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_7"
                            app:layout_constraintRight_toRightOf="parent"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:layout_marginTop="24dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_9"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_9"
                            app:layout_constraintHorizontal_chainStyle="spread_inside"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_10" />

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_10"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_10"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_9"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_11"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_11"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_11"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_10"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_12"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_12"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_12"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_11"
                            app:layout_constraintRight_toRightOf="parent"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:layout_marginTop="24dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_13"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_13"
                            app:layout_constraintHorizontal_chainStyle="spread_inside"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_14" />

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_14"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_14"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_13"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_15"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_15"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_15"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_14"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_16"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_16"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_16"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_15"
                            app:layout_constraintRight_toRightOf="parent"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:layout_marginTop="24dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_17"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_17"
                            app:layout_constraintHorizontal_chainStyle="spread_inside"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_18" />

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_18"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_18"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_17"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_19"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_19"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_19"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_18"
                            app:layout_constraintRight_toLeftOf="@id/fragment_history_choice_franchise_20"/>

                        <ImageView
                            android:id="@+id/fragment_history_choice_franchise_20"
                            android:layout_width="52dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/franchise_sample"
                            android:contentDescription="@string/image_view_description_choice_franchise_20"
                            app:layout_constraintLeft_toRightOf="@id/fragment_history_choice_franchise_19"
                            app:layout_constraintRight_toRightOf="parent"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="26dp"
                    android:text="@string/dialog_history_register_franchise_not_found"
                    android:textSize="18sp"
                    android:textColor="@color/color_text_grayish_brown"
                    android:fontFamily="@font/spoqa_han_sans_bold"
                    android:includeFontPadding="false"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="26dp"
                    android:text="@string/dialog_history_register_franchise_choice_custom_description"
                    android:textSize="14sp"
                    android:textColor="@color/color_text_brownish_grey"
                    android:fontFamily="@font/spoqa_han_sans_regular"
                    android:includeFontPadding="false"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/fragment_history_register_franchise_not_found_recycler_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="22dp"
                    android:layout_marginBottom="16dp"
                    android:nestedScrollingEnabled="false"
                    android:overScrollMode="never"
                    android:paddingStart="0dp"
                    android:paddingEnd="16dp"
                    android:clipToPadding="false"
                    tools:itemCount="5"
                    tools:listitem="@layout/list_item_coffee" />

            </LinearLayout>

        </ScrollView>

    </LinearLayout>

</layout>